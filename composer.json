{
  "name": "gwsn/csv-tools",
  "description": "Simple helpers for handling csv content",
  "license": "MIT",
  "homepage": "https://github.com/gwsn/csv-tools",
  "keywords": [
    "csv",
    "gwsn"
  ],
  "authors": [
    {
      "name": "Global Web Systems B.V.",
      "homepage": "https://www.globalwebsystems.nl"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "GWSN\\CsvTools\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UnitTest\\GWSN\\CsvTools\\": "tests"
    }
  },
  "scripts": {
    "test": [
      "Composer\\Config::disableProcessTimeout",
      "vendor/bin/phpunit"
    ],
    "unit-test": "@test"
  },
  "config": {
    "sort-packages": true
  }
}
